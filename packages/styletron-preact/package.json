    "prepublish": "npm run build"
  },
  "dependencies": {
    "styletron-client": "^3.0.2",
    "styletron-server": "^3.0.2",
    "styletron-utils": "^3.0.2"
  },
  "peerDependencies": {
    "preact": "6.x - 8.x"
